package cloudstudying;

public class pratice42 {

	public static int sum(int[]array) {
		int sum = 0;
		for(int i = 0; i < array.length; i++)
		sum += array[i];
		
		return sum;
	}
	
	public static void main(String[] args) {
 
		int n = 6;
