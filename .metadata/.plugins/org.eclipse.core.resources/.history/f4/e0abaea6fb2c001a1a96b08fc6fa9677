	public static void main(String[] args) {
		
	}

}

class circle01{
	
	double radius;
	double height;
	double PI = Double.parseDouble(String.format("%.2f",Math.PI));
	
	private double getVolume() {
		return radius*radius*PI;
	}
	private double getArea() {
		return 2*radius+radius*radius*PI;
	}
}