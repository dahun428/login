package cloudstudying;

public class pratice18 {

	int N;
	
	public void dice() {


	  N = (int) (6 * Math.random());
	  int temp = N + 1;
		
	  System.out.println(temp);
	}
 
	
	public static void main(String[] args) {
		
		pratice18 p18 = new pratice18();
		p18.dice();
		
