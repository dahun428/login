package cloudstudying;

public class pratice34 {

	public static void main(String[] args) {
 
		int sum = 0;
		for(int i = 1 ; i <=10 ; i++) {
			
			if(i % 3 == 0) {
				System.out.printf("%d", i);
				
				
				continue;
				
			}
			System.out.printf(" + ");
			

			sum += i;
		}
		System.out.printf("\n => %d", sum);
	}

}
