package cloudstudying03;

public class pratice03 {

	public static void main(String[] args) {
		
		Squre s1 = new Squre();
	}

}
class Squre{
	
	int lenght;
	public int area(int lenght) {
		this.lenght = lenght;
		return lenght * lenght;
	}
	public void areainfo() { 
		System.out.println(area(4));
	}
	
}