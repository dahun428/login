package cloudstudying;

public class pratice34 {

	public static void main(String[] args) {
 
		int sum = 0;
		for(int i = 0 ; i <=10 ; i++) {
			
			if(i % 3 == 0) {
				System.out.printf("%d", i);
				
				if( i == 10) {
				
					continue;

					System.out.printf(" + ");
					
				}
				continue;
				
				
			}
			

			sum += i;
		}
		System.out.printf("\n => %d", sum);
	}

}
