package cloudstudying;

public class pratice35 {

	static String x = "x";
	static int j;
	public static void main(String[] args) {
		
		for(int i = 2; i < 10; i++) {
		
			for(int j = 1; j <10; j++) {
				System.out.print(j);
			}
			
			System.out.printf ("%d %s %d", i, x, j );
		 
			System.out.println(" = ");
			 
		}
 
	}

}
