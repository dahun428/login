package cloudstudying;

public class pratice34 {

	public static void main(String[] args) {
 
		int sum = 0;
		for(int i = 0 ; i <=10 ; i++) {
			
			if(i % 3 == 0) {
				System.out.printf("%d", i);
				 System.out.printf(" + ");
				 
				continue;
			}
			if(i == 10) {
			break;
			}
			

			sum += i;
		}
		System.out.printf("\n => %d", sum);
	}

}
