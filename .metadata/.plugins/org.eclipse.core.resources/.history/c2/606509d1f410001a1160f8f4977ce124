package cloudstudying;

public class pratice30 {
	
	public static void countDown( ) {
		System.out.println("카운트다운을 시작합니다.");
		
		int i = 4;
		
		while ( i > 0) {
			System.out.printf("하나 : %d\n",i);
			i--;
		}
		System.out.println("발사");
	}
	
	public static void main(String[] args) {
  
		countDown();
		
			
		}
		
	 

}
