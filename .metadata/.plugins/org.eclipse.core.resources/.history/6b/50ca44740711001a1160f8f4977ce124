package cloudstudying;

public class pratice34 {

	public static void main(String[] args) {
 
		int sum = 0;
		for(int i = 0 ; i <=10 ; i++) {
			System.out.printf("%d", i);
			if(i % 3 == 0) {
				continue;
			}
			sum += i;
			System.out.printf(" + ");
		}
		System.out.printf("\n => %d", sum);
	}

}
